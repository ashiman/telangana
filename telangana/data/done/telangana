       $('th.ms-vh2 .ms-vh-div').removeAttr('filterdisablemessage'); 
       $('th.ms-vh2 .ms-vh-div').removeAttr('ctxnum'); 
       $('th.ms-vh2 .ms-vh-div').removeAttr('displayname'); 
       $('th.ms-vh2 .ms-vh-div').removeAttr('fieldtype'); 
       $('th.ms-vh2 .ms-vh-div').removeAttr('resulttype'); 
       $('th.ms-vh2 .ms-vh-div').removeAttr('sortfields'); 
